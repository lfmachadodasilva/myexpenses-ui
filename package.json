    "scripts": {
        "start": "react-scripts start",
        "start:https": "HTTPS=true react-scripts start",
        "start:https:win": "set HTTPS=true&&react-scripts start",
        "start:silent": "BROWSER=none react-scripts start",
        "build": "react-scripts build --if-present",
        "build:github": "react-scripts build --base-href https://lfmachadodasilva.github.io/myexpenses-ui/ && cp build/index.html build/404.html",
        "test": "react-scripts test --watchAll=false",
        "test:coverage": "react-scripts test --watchAll=false --coverage",
        "eject": "react-scripts eject",
        "deploy": "gh-pages -d build",
        "flow": "flow",
        "ui-tests": "start-server-and-test start:silent http://localhost:3000 cy:run",
        "cy:run": "cypress run"
    },
    "eslintConfig": {
        "extends": "react-app",
        "rules": {
            "jsx-a11y/anchor-is-valid": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
