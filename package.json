{
    "name": "myexpenses-ui",
    "homepage": "https://lfmachadodasilva.github.io/myexpenses-ui",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.19.2",
        "bootstrap": "^4.3.1",
        "chart.js": "^2.9.3",
        "chartjs-plugin-colorschemes": "^0.4.0",
        "date-fns": "^2.12.0",
        "firebase": "^7.13.2",
        "i18next": "^19.4.0",
        "lodash": "^4.17.15",
        "query-string": "^6.12.0",
        "react": "^16.13.1",
        "react-bootstrap": "^1.0.0",
        "react-chartjs-2": "^2.9.0",
        "react-datepicker": "^2.14.1",
        "react-device-detect": "^1.11.14",
        "react-dom": "^16.13.1",
        "react-firebase-hooks": "^2.1.1",
        "react-global-configuration": "^1.4.0",
        "react-i18next": "^11.3.4",
        "react-icons": "^3.9.0",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.4.1",
        "react-select": "^3.1.0",
        "typescript": "^3.8.3"
    },
    "devDependencies": {
        "@testing-library/react": "^9.5.0",
        "@types/chart.js": "^2.9.19",
        "@types/jest": "^24.0.23",
        "@types/lodash": "^4.14.149",
        "@types/node": "^12.12.7",
        "@types/react": "^16.9.11",
        "@types/react-datepicker": "^2.11.0",
        "@types/react-dom": "^16.9.4",
        "@types/react-global-configuration": "^1.3.1",
        "@types/react-router-dom": "^5.1.2",
        "@types/react-select": "^3.0.11",
        "eslint-plugin-flowtype": "^4.7.0",
        "gh-pages": "^2.2.0",
        "start-server-and-test": "^1.10.11"
    },
    "scripts": {
        "start": "react-scripts start",
        "start:https": "HTTPS=true react-scripts start",
        "start:https:win": "set HTTPS=true&&react-scripts start",
        "start:silent": "BROWSER=none react-scripts start",
        "build": "react-scripts build --if-present",
        "build:github": "react-scripts build --base-href https://lfmachadodasilva.github.io/myexpenses-ui/ && cp build/index.html build/404.html",
        "test": "react-scripts test --watchAll=false",
        "test:coverage": "react-scripts test --watchAll=false --coverage",
        "eject": "react-scripts eject",
        "deploy": "gh-pages -d build",
        "flow": "flow",
        "ui-tests": "start-server-and-test start:silent http://localhost:3000 cy:run",
        "cy:run": "cypress run"
    },
    "eslintConfig": {
        "extends": "react-app",
        "rules": {
            "jsx-a11y/anchor-is-valid": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
